  "scripts": {
    "dev": "storybook dev -p 6006",
    "dev:docs": "DOCS_MODE=true pnpm run dev",
    "dev:coder": "pnpm build --watch -- --buildSync",
    "start": "pnpm run dev",
    "size": "pnpm run build && size-limit",
    "build": "tsc --project ./tsconfig-build.json && run-p build-icons && BUILD_TYPES=true vite build -- --tailwind",
    "build:tsup": "tsup && pnpm run build:tailwind && run-p build-icons",
    "build-icons": "tsc --project ./tsconfig-build-icons.json",
    "build:tailwind": "NODE_ENV=production tailwindcss -i src/styles.css -o ./dist/styles.css --postcss --minify",
    "build:watch": "pnpm build --watch",
    "lint": "eslint -c eslint.config.mjs src/**/*.{ts,tsx} --report-unused-disable-directives --max-warnings 0",
    "lint-fix": "eslint -c eslint.config.mjs src/**/*.{ts,tsx} --fix --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "generate-icons": "rm -fR src/icons/app/* src/icons/modules/* && run-p generate-app-icons generate-module-icons",
    "generate-app-icons": "npx @svgr/cli --out-dir src/icons/app node_modules/@factorialco/f0-core/assets/icons/app",
    "generate-module-icons": "npx @svgr/cli --out-dir src/icons/modules node_modules/@factorialco/f0-core/assets/icons/modules",
    "build-storybook": "storybook build",
    "serve-storybook": "http-server storybook-static",
    "test-storybook": "test-storybook --testTimeout=60000",
    "prettier:format": "prettier --write \"src/**/*.{ts,tsx,js,md,mdx,css,yaml}\"",
    "prettier:check:ci": "prettier --check \"src/**/*.{ts,tsx,js,md,mdx,css,yaml}\"",
    "tsc": "tsc --noEmit",
    "vitest": "vitest",
    "vitest:ui": "vitest --ui",
    "vitest:watch": "vitest dev",
    "vitest:ci": "pnpm run vitest --reporter=dot"
  },
