{
  "name": "@factorialco/f0-core",
  "version": "1.26.0",
  "description": "Core tokens and utilities for F0 Design System",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "development": "./src/index.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./base.css": "./base.css",
    "./tailwind": "./tailwind.ts",
    "./package.json": "./package.json",
    "./assets/icons/modules/*": "./assets/icons/modules/*",
    "./assets/icons/modules": "./assets/icons/modules/index.ts",
    "./assets/icons/app/*": "./assets/icons/app/*",
    "./assets/icons/app": "./assets/icons/app/index.ts"
  },
  "files": [
    "src",
    "dist",
    "assets",
    "base.css",
    "tailwind.ts"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint . --ext .ts,.tsx"
  },
  "keywords": [
    "factorial",
    "design-system",
    "tokens"
  ],
  "author": "Factorial",
  "license": "MIT",
  "peerDependencies": {
    "@tailwindcss/container-queries": "^0.1.1",
    "tailwindcss": "^3.4.3",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.7.2"
  },
  "devDependencies": {
    "prettier": "^3.5.2",
    "tsup": "^8.3.5"
  }
}
